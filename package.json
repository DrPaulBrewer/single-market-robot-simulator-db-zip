{
  "name": "single-market-robot-simulator-db-zip",
  "version": "2.1.0",
  "description": "readonly studyfolder class for zip file blobs",
  "main": "build/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel src --out-dir build",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/**.js",
    "go": "npm run clean && npm run lint && npm run build"
  },
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drpaulbrewer/single-market-robot-simulator-db-zip"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "esdoc": "^1.1.0",
    "eslint": "^6.8.0"
  },
  "dependencies": {
    "single-market-robot-simulator-db-studyfolder": "^2.0.0"
  }
}
