  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drpaulbrewer/single-market-robot-simulator-db-zip"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "esdoc": "^1.1.0",
    "eslint": "^6.6.0"
  },
  "dependencies": {
    "single-market-robot-simulator-db-studyfolder": "^1.1.0"
  }
}
