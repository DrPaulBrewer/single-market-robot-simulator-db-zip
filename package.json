{
  "name": "single-market-robot-simulator-db-zip",
  "version": "1.2.0",
  "description": "readonly studyfolder class for zip file blobs",
  "main": "build/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel src --out-dir build ; done",
